{
  "version": 2,
  "functions": {
    "api/socket.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "game/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/socket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socket"
    },
    {
      "src": "/(.*)",
      "dest": "/game/$1"
    }
  ],
  "headers": [
    {
      "source": "/game/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/game/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}