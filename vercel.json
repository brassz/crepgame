{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/multiplayer-demo.html"
    },
    {
      "source": "/game",
      "destination": "/game/index.html"
    },
    {
      "source": "/game/(.*)",
      "destination": "/game/$1"
    },
    {
      "source": "/bronze",
      "destination": "/game/index.html?room=bronze"
    },
    {
      "source": "/prata",
      "destination": "/game/index.html?room=prata"
    },
    {
      "source": "/ouro",
      "destination": "/game/index.html?room=ouro"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/game/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/game/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/game/sounds/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/game/sprites/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}