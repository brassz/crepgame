{
  "version": 2,
  "builds": [
    {
      "src": "game/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socket"
    },
    {
      "src": "/game/(.*)",
      "dest": "/game/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/game/$1"
    }
  ],
  "functions": {
    "api/socket.js": {
      "maxDuration": 30
    }
  }
}